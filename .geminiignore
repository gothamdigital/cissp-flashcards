# Gemini CLI ignore file
# This file specifies intentionally untracked files that Gemini CLI should ignore.
# It follows the same patterns as .gitignore.

# Dependency directories
node_modules/

# Build artifacts
dist/
build/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Environment variables
.env
.env.local
.env.*.local

# Gemini CLI internal files
tasks/
.gemini/

# Lock files
package-lock.json
yarn.lock
pnpm-lock.yaml